{
    "presets": [
        "es2015-rollup",
        "flow",
        "stage-3"
    ],
    "plugins": [
        [
            "transform-class-properties",
            {
                "spec": true
            }
        ],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}